struct _keyrange {
    string low
    string high
}

service _Chord {
    fetch_lookup_table( | map<_keyrange, string>)
    migrate_some_range(string dst_addr | i32 err)
    start_migration(_keyrange | i32 err)
    finish_migration(_keyrange | i32 err)
    leave( | )
    debug_print( | )

    get(string key | i32 err, string value)
    put(string key, string value | i32 err)
    remove(string key | i32 err)
}
